{
    "diffEditor.ignoreTrimWhitespace": false,
    "diffEditor.renderSideBySide": true,
    "editor.fontFamily": "'JetBrains Mono', Menlo, Monaco, 'Courier New', monospace",
    "editor.renderWhitespace": "all",
    "editor.suggestSelection": "first",
    "editor.minimap.enabled": false,
    "editor.renderControlCharacters": false,
    "editor.occurrencesHighlight": false,
    "editor.selectionHighlight": true,
    "editor.formatOnType": true,
    "extensions.ignoreRecommendations": true,
    "editor.scrollbar.verticalScrollbarSize": 12,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "files.autoSave": "afterDelay",
    "git.enableSmartCommit": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "githubPullRequests.fileListLayout": "tree",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "terminal.integrated.fontFamily": "'JetBrains Mono'",
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.cwd": "${workspaceFolder}",
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.rightClickBehavior": "copyPaste",
    "terminal.integrated.inheritEnv": false,
    "workbench.sideBar.location": "left",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.startupEditor": "newUntitledFile",
    "workbench.activityBar.visible": true,
    "workbench.statusBar.visible": true,
    "workbench.colorTheme": "CodeSandbox",
    "workbench.colorCustomizations": {
        "terminal.selectionBackground": "#c0c0c071",
        "[CodeSandbox]": {
            "button.background": "#528bff",
            "button.foreground": "#ffffff",
            "button.hoverBackground": "#6087cf",
            "list.focusBackground": "#655d6a85",
            "list.hoverBackground": "#555d6b85",
            "editorGroup.dropBackground": "#98afd80b",
            "editor.lineHighlightBackground": "#3d3e413a",
            "editor.foldBackground": "#3d3e415a",
            "peekViewEditor.matchHighlightBackground": "#87878764",
            "activityBar.background": "#40494d80",
            "problemsWarningIcon.foreground": "#cca700",
            "badge.background": "#528bff",
            "badge.foreground": "#d7dae0",
            "scrollbarSlider.background": "#ffffff20",
            "scrollbarSlider.hoverBackground": "#ffffff40",
            "scrollbarSlider.activeBackground": "#ffffff60",
            "statusBar.noFolderBackground": "#40494d80",
            "statusBar.background": "#3491b9c4"
        }
    }
}